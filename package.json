{
  "name": "inferno-website",
  "version": "1.0.0",
  "description": "The official website for Inferno - a JavaScript library for creating UIs on the web",
  "main": "webpack.config.babel.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/src/server/server.js",
    "dev": "cross-env NODE_ENV=development nodemon index.js",
    "prod": "cross-env NODE_ENV=production node index.js",
    "build": "cross-env NODE_ENV=production node core/compile.js && rm -fr dist && mkdir -p dist && mkdir -p dist/src && mkdir -p dist/public && mkdir -p dist/assets && mkdir -p dist/core && cp -r src/* dist/src && cp -r public/* dist/public && cp -r core/* dist/core && NODE_ENV=development BABEL_ENV=development npx babel src --out-dir dist/src",
    "now-build": "npm run build",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infernojs/inferno-website.git"
  },
  "keywords": [
    "inferno",
    "website"
  ],
  "author": "Dominic Gannaway",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/infernojs/inferno-website/issues"
  },
  "homepage": "https://github.com/infernojs/inferno-website#readme",
  "dependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-transform-async-to-generator": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "@babel/cli": "^7.4.4",
    "@intervolga/optimize-cssnano-plugin": "^1.0.6",
    "babel-loader": "8.0.6",
    "babel-plugin-inferno": "6.0.5",
    "bootstrap": "4.3.1",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "^2.0.2",
    "commonmark": "^0.28.1",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^2.1.1",
    "debug": "^4.1.1",
    "eslint-plugin-inferno": "7.11.0",
    "fast-async": "^7.0.6",
    "file-loader": "3.0.1",
    "history": "^4.9.0",
    "html-loader": "^0.5.5",
    "http2": "^3.3.7",
    "inferno": "7.1.12",
    "inferno-create-class": "7.1.12",
    "inferno-create-element": "7.1.12",
    "inferno-devtools": "7.1.12",
    "inferno-hydrate": "^7.1.12",
    "inferno-router": "7.1.12",
    "inferno-server": "7.1.12",
    "inferno-vnode-flags": "7.1.12",
    "isomorphic-fetch": "2.2.1",
    "json-loader": "0.5.7",
    "jwt-simple": "^0.5.6",
    "koa": "^2.7.0",
    "koa-better-body": "^3.0.4",
    "koa-compress": "^3.0.0",
    "koa-convert": "^1.2.0",
    "koa-favicon": "^2.0.1",
    "koa-mount": "^4.0.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "markdown-loader": "^5",
    "mini-css-extract-plugin": "^0.5.0",
    "mz": "^2.7.0",
    "node-sass": "4.12.0",
    "prismjs": "^1.16.0",
    "sass-loader": "7.1.0",
    "serve-favicon": "2.5.0",
    "spdy": "^4.0.0",
    "style-loader": "0.23.1",
    "supports-color": "^6.1.0",
    "sw-precache-webpack-plugin": "^0.11.5",
    "webpack": "^4",
    "xss-filters": "^1.2.7"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "cross-env": "5.2.0",
    "eslint": "^5.16.0",
    "nodemon": "1.19.1",
    "webpack-dev-server": "^3",
    "prettier": "1.17.1"
  }
}
